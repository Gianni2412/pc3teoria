@model Producto



<h1>Nuevo Producto</h1>

<form action=" " method="post">

    <div class="form-group">
        <label asp-for="Nombre"></label>
        <input asp-for="Nombre" type="text" class="form-control">

        <label asp-for="Foto"></label>
        <input asp-for="Foto" type="text" class="form-control">

    </div>


    <div class="form-group">
        <label asp-for="Categoria"></label>
        <select asp-for="CategoriaId" asp-items="ViewBag.Categoria" type="text" class="form-control"></select>

    </div>


    <button class="btn btn-primary">Registrar</button>





</form>