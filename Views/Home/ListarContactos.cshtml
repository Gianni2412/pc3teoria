@model List<Usuario>


<h1>LISTA DE CONTACTOS</h1>

<table class="table table-dark">
    <tr>
        <th>ID</th>
        <th>Nombres</th>
        <th>celular</th>
        <th>lugar</th>


    </tr>

    @foreach (var u in Model)
    {
        <tr>
            <td>@u.id</td>
            <td>@u.Nombre</td>
            <td>@u.Celular</td>
            <td>@u.Lugar</td>

        </tr>
    }


</table>


